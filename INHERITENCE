#include <iostream>
using namespace std;

// Base class
class Animal {
public:
    void sound() {
        cout << "Animals make sounds." << endl;
    }
};

// Derived class
class Dog : public Animal {
public:
    void bark() {
        cout << "Dog barks: Woof!" << endl;
    }
};

int main() {
    Dog myDog;
    myDog.sound();  // Inherited from Animal
    myDog.bark();   // Defined in Dog
    return 0;
}
